{
  "name": "cabag/ext-cabag-loginas",
  "description": "CAB Login As",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4",
    "php": "^8.2 || ^8.3"
  },
  "autoload": {
    "psr-4": {
      "Cabag\\CabagLoginas\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "cabag_loginas"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "replace": {
    "cabag/ext-cabag-loginas": "self.version"
  }
}
